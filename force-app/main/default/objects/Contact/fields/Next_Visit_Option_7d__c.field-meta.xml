<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Next_Visit_Option_7d__c</fullName>
    <description>The next visit 7d past Previous to Last Visit</description>
    <externalId>false</externalId>
    <formula>IF (OR(ISNULL(Last_Previous_Donation__r.Donation_Complete__c), ISNULL(Last_Previous_Scheduled_Visit__r.Appointment_Datetime__c)), 
   IF (ISNULL(Last_Previous_Donation__r.Donation_Complete__c),
      Last_Previous_Scheduled_Visit__r.Appointment_Datetime__c + 7,
      Last_Previous_Donation__r.Donation_Complete__c + 7
   ),
   FROMUNIXTIME(MAX(UNIXTIMESTAMP(Last_Previous_Donation__r.Donation_Complete__c + 7), UNIXTIMESTAMP(Last_Previous_Scheduled_Visit__r.Appointment_Datetime__c + 7)))
)</formula>
    <label>Next Visit Option 7d</label>
    <required>false</required>
    <type>DateTime</type>
</CustomField>
