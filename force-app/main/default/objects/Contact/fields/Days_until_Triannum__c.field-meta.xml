<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_until_Triannum__c</fullName>
    <description>The number of days until a donor&apos;s next First Visit triannum (q4months)</description>
    <externalId>false</externalId>
    <formula>MIN ( 
ABS(Trianniversary_1__c - TODAY()), ABS(Trianniversary_2__c - TODAY())
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of days until a donor&apos;s next First Visit triannum (q4months)</inlineHelpText>
    <label>Days until Triannum</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
