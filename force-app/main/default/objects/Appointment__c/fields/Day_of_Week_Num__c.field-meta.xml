<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Day_of_Week_Num__c</fullName>
    <description>Day of the week enumerator (Sunday=1...Saturday=7)</description>
    <externalId>false</externalId>
    <formula>CASE(WEEKDAY(  DATEVALUE(Datetime__c)  ), 
1, &quot;Sunday&quot;, 
2, &quot;Monday&quot;, 
3, &quot;Tuesday&quot;, 
4, &quot;Wednesday&quot;, 
5, &quot;Thursday&quot;, 
6, &quot;Friday&quot;, 
7, &quot;Saturday&quot;, 
&quot;Monday&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Day of Week Num</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
